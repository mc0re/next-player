Public Enum SoundPositionModes

	''' <summary>
	''' No position change.
	''' </summary>
	FixedToChannels


	''' <summary>
	''' The position shifts between the channels by a single value (panning).
	''' </summary>
	Panning


	''' <summary>
	''' The position is defined explicitly by the coordinates.
	''' </summary>
	Coordinates

End Enum
