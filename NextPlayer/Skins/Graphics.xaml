<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


	<!--#region Pens for standard image size 100x100 -->

	<Pen x:Key="RoundButtonImageRoundPen"
		 Brush="{DynamicResource ButtonTextBrush}"
		 Thickness="20" LineJoin="Round"
		 EndLineCap="Round" StartLineCap="Round"/>

	
	<Pen x:Key="RoundButtonImageSquarePen"
		 Brush="{DynamicResource ButtonTextBrush}"
		 Thickness="20" LineJoin="Miter"
		 EndLineCap="Square"  StartLineCap="Square"/>

	
	<Pen x:Key="IconOutlinePen"
		 Brush="{DynamicResource ButtonTextBrush}" LineJoin="Round"
		 EndLineCap="Round" StartLineCap="Round"/>

	
	<Pen x:Key="PlaylistIconPen"
		 Brush="{DynamicResource ActionButtonPlainBrush}"
		 Thickness="20" LineJoin="Round"
		 EndLineCap="Round" StartLineCap="Round"/>

	
	<Pen x:Key="ActionButtonImagePen"
		 Brush="{DynamicResource RectButtonTextBrush}"
		 Thickness="20" LineJoin="Round"
		 EndLineCap="Round" StartLineCap="Round"/>

	
	<Pen x:Key="ErrorPen"
		 Brush="{DynamicResource AttentionBrush}"
		 Thickness="20" LineJoin="Round"
		 EndLineCap="Round" StartLineCap="Round"/>


	<Pen x:Key="ExecutionTypePen"
		 Brush="{DynamicResource AttentionBrush}"
		 Thickness="20" LineJoin="Miter"
		 EndLineCap="Square" StartLineCap="Square"/>


	<Pen x:Key="EffectThinPen"
		 Brush="{DynamicResource EffectPlainBrush}"
		 Thickness="1" LineJoin="Miter"
		 EndLineCap="Square" StartLineCap="Square"/>

	<!--#endregion-->
    
    
